<p id="notice"><%= notice %></p>
<p>
  <strong>Name:</strong>
  <%= @user.name %>
  <strong>Events created:</strong>
  <h5>Future Events</h5>
  <% @upcoming_events.each do |event|%>
  <%= event.description %>
  <% end %>
  <h5>Past Events</h5>
  <% @previous_events.each do |event| %>
    <%= event.description%>
  <% end %>
  <strong>Events Attended:</strong>
  <% @user.attended_events.each do |event|%>
  <%= event.description %>
  <% end %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
